{
  "expo": {
    "name": "2y4Race",
    "slug": "car-marketplace-app",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "car-marketplace-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#3D9970"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carmarketplace.app"
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.muotrimarcelo.carmarketplaceapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "app.2y4race.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3df5b46b-ba50-4962-93a8-edd5f329ecbc"
      }
    }
  }
}
